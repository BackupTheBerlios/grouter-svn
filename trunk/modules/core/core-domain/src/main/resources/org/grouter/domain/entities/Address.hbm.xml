<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="org.grouter.domain.entities.Address" table="address">
        <id column="id" name="id" type="java.lang.String">
            <generator class="uuid"/>
        </id>
        <property name="phone"  type="java.lang.String" column="phone"/>
        <property name="mobilephone"  type="java.lang.String" column="mobilephone"/>
        <property name="street" type="java.lang.String" column="street"/>
        <property name="zip" type="java.lang.String" column="zip"/>
        <property name="city" type="java.lang.String" column="city"/>
        <property name="fax" type="java.lang.String" column="fax"/>
        <property name="homepageUrl" type="java.lang.String" column="homepageurl"/>
        <property name="companyname"  type="java.lang.String" column="companyname"/>
        <property name="email" type="java.lang.String" column="email"/>
    </class>
</hibernate-mapping>
