<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd"
  default-autowire="byName">


  <bean id="org.grouter" class="org.springframework.context.support.ClassPathXmlApplicationContext">
    <constructor-arg>
      <list>
        <value>context-domain-dao.xml</value>
        <value>context-domain-datasource.xml</value>
        <!-- value>context-domain-service.xml</value -->
        <value>context-domain-sessionfactory.xml</value>
      </list>
    </constructor-arg>
  </bean>

</beans>