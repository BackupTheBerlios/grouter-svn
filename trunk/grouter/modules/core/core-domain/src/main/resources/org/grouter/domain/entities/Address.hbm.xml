<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="org.grouter.domain.entities.Address" table="address">
        <id column="id" name="id" type="java.lang.String">
            <generator class="uuid"/>
        </id>
        <property name="address" not-null="false" type="java.lang.String" column="address"/>
        <property name="name" not-null="false" type="java.lang.String" column="name"/>
    </class>
</hibernate-mapping>
