<?xml version="1.0" encoding="UTF-8"?>
<xml-fragment name="Stored new name" xsi:schemaLocation="http://grouter.org/config.xsd ../../xsd/config.xsd" xsi:noNamespaceSchemaLocation="../../main/xsd/config.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <con:Global xmlns:con="http://grouter.org/config.xsd">
        <con:ArchiveHandler>
            <!--xs:enumeration value="Every 10 seconds@0/10 * * * * ?"/>
                   <xs:enumeration value="Every 10 minutes@* 0/10 * * * ?"/>
                   <xs:enumeration value="Every 30 seconds@* 0/30 * * * ?"/>
                   <xs:enumeration value="Every 1 minute@0 0 * * * ?"/>
                   <xs:enumeration value="Every 60 minutes@* 1 * * * ?"/>
                   <xs:enumeration value="Every 10 minutes@0/10 * * * * ?"/-->
            <con:CronJob>Every 10 minutes@0 0 0-23 * * ?</con:CronJob>
        </con:ArchiveHandler>
        <con:Email sendintervallInMilliSeconds="10000">
            <con:smtp>
                <con:smtpserver id="1">mailserverl</con:smtpserver>
                <con:smtpserver id="2">mailserver2</con:smtpserver>
            </con:smtp>
            <con:pop>
                <con:popserver id="1">mailserverl</con:popserver>
                <con:popserver id="2">mailserver2</con:popserver>
            </con:pop>
            <con:imap>
                <con:imapserver id="1">mailserverl</con:imapserver>
                <con:imapserver id="2">mailserver2</con:imapserver>
            </con:imap>
        </con:Email>
    </con:Global>
    
    <con:Node type="file-file" id="filetest-filetest" createuniquename="true" useRelativeRootpath="false" xmlns:con="http://grouter.org/config.xsd">
        <con:inFolderConfig>
            <con:inFolderPath>in</con:inFolderPath>
            <con:pollIntervallMilliSeconds>5000</con:pollIntervallMilliSeconds>
            <con:skipFirstblankline>false</con:skipFirstblankline>
            <con:batchReadConfig isBatchOn="false">
                <con:batchSize>10</con:batchSize>
                <con:batchThreshold>10</con:batchThreshold>
            </con:batchReadConfig>
        </con:inFolderConfig>
        <con:outFolderConfig>
            <con:outFolderPath>out</con:outFolderPath>
        </con:outFolderConfig>
        <con:backup backupHandlerOn="true" archiveroOn="true">
            <con:backupFolderPath>backup/</con:backupFolderPath>
            <con:archiveFolderPath>archived/</con:archiveFolderPath>
        </con:backup>
        <con:transform transformOn="false"/>
        <con:error errorHandlerOn="true">
            <con:errorFolderPath>error/</con:errorFolderPath>
            <con:errorInFolderPath>error_info</con:errorInFolderPath>
            <con:sortOnResend>false</con:sortOnResend>
            <con:CronJob>Every minute@0/20 * * * * ?</con:CronJob>
            <con:errorNotification errorNotificationOn="true">
                <con:notifyEveryIntervallInMilliSeconds>180000</con:notifyEveryIntervallInMilliSeconds>
                <con:numberOfErrors>1</con:numberOfErrors>
                <con:emailNotification>
                    <con:emailOnFailure>/template/emailerrornotification.xml</con:emailOnFailure>
                </con:emailNotification>
                <con:sms>
                    <con:smsOnFailure>/template/down.xml</con:smsOnFailure>
                    <con:smsService>/in</con:smsService>
                </con:sms>
            </con:errorNotification>
        </con:error>
        <con:inactivity inactivityOn="false">
            <con:maxInactivityPeriodInMilliSeconds>120000</con:maxInactivityPeriodInMilliSeconds>
            <con:emailOnInactivity>/template/erroronincativity.xml</con:emailOnInactivity>
        </con:inactivity>
        <!--applicationlogger>
            <appender type="RollingFileAppender">
                <maxfilesize>100KB</maxfilesize>
                <maxbackupindex>10</maxbackupindex>
            </appender>
            <logpattern>%d{yyyy-MM-dd HH:mm:ss};%p;%t;%x;%m;%n</logpattern>
            <logfolderpath>/log</logfolderpath>
            <loglevel>DEBUG</loglevel>
        </applicationlogger>
        <messagelogger>
            <appender type="DailyRollingFileAppender">
                <rollingschedule>'.'yyyy-MM-dd-a</rollingschedule>
            </appender>
            <logpattern>%d{yyyy-MM-dd HH:mm:ss};%p;%t;%x;%m;%n</logpattern>
            <logfolderpath>/log</logfolderpath>
            <loglevel>DEBUG</loglevel>
        </messagelogger-->
    </con:Node>
</xml-fragment>