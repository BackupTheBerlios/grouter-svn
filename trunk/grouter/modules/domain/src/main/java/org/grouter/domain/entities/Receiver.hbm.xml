<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping SYSTEM
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    <class name="org.grouter.domain.entities.Receiver" table="RECEIVER">
        <id column="ID" name="id" type="java.lang.String">
            <generator class="uuid"/>
        </id>
        <!-- "transactional|read-write|nonstrict-read-write|read-only -->
        <!-- cache usage="read-only" / -->
        <property name="name" not-null="false" type="java.lang.String" column="NAME"/>
        <!--many-to-one
            name="commandMessages"
            not-null="true"
            class="Message"
            column="MESSAGEID"
            cascade="none">
        </many-to-one-->

        
        <set name="messages" table="RECEIVER_MESSAGE" >
            <key column="RECEIVER_ID"/>
            <many-to-many class="org.grouter.domain.entities.Message" column="MESSAGE_ID"/>
        </set>

    </class>
</hibernate-mapping>
